{
	"image": "ghcr.io/anybody/anybodycon:7.4.4-1",
  "hostRequirements": {
    "cpus": 4
  },
	"features": {
		"ghcr.io/devcontainers/features/common-utils:2": {},
		"ghcr.io/devcontainers/features/github-cli:1": {},
		"ghcr.io/mamba-org/devcontainer-features/micromamba:1": {
			"channels": "conda-forge",
			"packages": "python>=3.11 anypytools jupyterlab ipywidgets ipykernel matplotlib tqdm pandas"
		  }
	},
  "waitFor": "onCreateCommand",
  "postCreateCommand": "",
  "customizations": {
    "codespaces": {
      "openFiles": []
    },
    "vscode": {
      "extensions": [
        "ms-toolsai.jupyter",
        "ms-python.python",
      ]
    }
  }
}
